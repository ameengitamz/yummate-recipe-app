{
  "name": "yummate-recipe-app",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SPOONACULAR_API_KEY": "@vite_spoonacular_api_key",
    "VITE_SPOONACULAR_BASE_URL": "@vite_spoonacular_base_url",
    "VITE_APP_NAME": "@vite_app_name",
    "VITE_APP_VERSION": "@vite_app_version"
  }
}
